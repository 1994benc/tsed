    "@types/superagent": "4.1.4",
    "@types/supertest": "2.0.8",
    "@types/swagger-schema-official": "2.0.20",
    "@types/graphql": "14.5.0",
    "ajv": "6.10.2",
    "apollo-datasource-rest": "0.6.6",
    "apollo-server-express": "2.9.9",
    "body-parser": "1.19.0",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chalk": "3.0.0",
    "compression": "1.7.4",
